{
  "name": "reactotron-realm",
  "version": "1.0.0",
  "description": "A Reactotron plugin for realm.",
  "author": "lehaiquantb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lehaiquantb/reactotron-realm/issues"
  },
  "homepage": "https://github.com/lehaiquantb/reactotron-realm",
  "repository": "https://github.com/lehaiquantb/reactotron-realm",
  "files": [
    "dist",
    "LICENSE"
  ],
  "main": "dist/reactotron-realm.umd.js",
  "module": "dist/reactotron-realm.es5.js",
  "typings": "dist/types/reactotron-realm.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch --notify",
    "format": "prettier --write {**,.}/*.ts",
    "build": "npm-run-all clean tsc compile",
    "build:dev": "npm-run-all clean tsc compile:dev",
    "clean": "trash dist",
    "lint": "eslint src --ext .ts,.tsx",
    "compile": "NODE_ENV=production rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "compile:dev": "NODE_ENV=development rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "tsc": "tsc",
    "ci:test": "yarn test",
    "ci:publish": "yarn semantic-release",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "reactotron-core-client": "^2.5.0",
    "realm": "^10.24.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-typescript": "^7.21.5",
    "@rollup/plugin-typescript": "^11.1.1",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^29.5.0",
    "lodash.camelcase": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "reactotron-core-client": "^2.8.10",
    "realm": "^10.24.0",
    "rollup": "^3.23.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-babel-minify": "^10.0.0",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-resolve": "^0.0.1-predev.1",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "19.0.5",
    "testdouble": "^3.18.0",
    "trash-cli": "^5.0.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4"
  }
}
